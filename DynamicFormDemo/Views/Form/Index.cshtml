@model IDataResult<IQueryable<Form>>
@using Core.Utilities.Results
@using DynamicFormDemo.Entity.Concrete

@{
    ViewData["Title"] = "Index";
    Layout = "~/Pages/Shared/_UserLayout.cshtml";
}

<table class="table mt-3">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Description</th>
            <th scope="col">Created By</th>
            <th scope="col">Created Date</th>
            <th scope="col">Detail</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Data)
        {
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.Name</td>
                <td>@item.Description</td>
                <td>@item.User.UserName</td>
                <td>@item.CreatedAt.ToString("dd MMMM yyyy")</td>
                <td><a class="btn btn-info" href="Forms/@item.Id/">Detail</a></td>
            </tr>
        }

    </tbody>
</table>
